{
  "env": {
    "es2022": true,
    "node": true,
    "mocha": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "plugins": ["@typescript-eslint", "mocha-no-only"],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "mocha-no-only/mocha-no-only": ["error"],
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": ["error"]
  }
}
